{
  "extends": "./tsconfig.settings",
  "include": ["./next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["./node_modules"],
  "compilerOptions": {
    "noEmit": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  }
}
