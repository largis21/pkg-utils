{
  "type": "module",
  "name": "@sanity/pkg-utils",
  "version": "1.8.2",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "description": "Simple utilities for modern npm packages.",
  "keywords": [
    "sanity-io",
    "sanity",
    "npm",
    "package",
    "utilities",
    "build",
    "bundling"
  ],
  "source": "./src/node/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/src/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/node/index.d.ts",
      "source": "./src/node/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "pkg": "./bin/pkg-utils.cjs",
    "pkg-utils": "./bin/pkg-utils.cjs"
  },
  "files": [
    "bin",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:pkg check:pkg",
    "build:pkg": "node --experimental-loader ./scripts/_experimental-loader.js --no-warnings -r esbuild-register scripts/build",
    "check:pkg": "node --experimental-loader ./scripts/_experimental-loader.js --no-warnings -r esbuild-register scripts/check",
    "check:types": "tsc --build",
    "clean": "rimraf dist test/env/__tmp__",
    "commit": "cz",
    "extract": "sanity-tsdoc etl --outDir etc/api/@sanity/pkg-utils --tsconfig tsconfig.dist.json",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint . --ext .cjs,.js,.ts,.tsx",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release",
    "test": "vitest run",
    "watch": "node --experimental-loader ./scripts/_experimental-loader.js --no-warnings -r esbuild-register scripts/watch"
  },
  "dependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.19.0",
    "@microsoft/api-extractor": "^7.29.5",
    "@microsoft/tsdoc-config": "^0.16.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "browserslist": "^4.21.3",
    "cac": "^6.7.12",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "esbuild": "^0.15.5",
    "esbuild-register": "^3.3.3",
    "find-config": "^1.0.0",
    "globby": "^11.1.0",
    "jsonc-parser": "^3.1.0",
    "mkdirp": "^1.0.4",
    "pkg-up": "^3.1.0",
    "prettier": "^2.7.1",
    "pretty-bytes": "^5.6.0",
    "rollup": "^2.78.1",
    "rollup-plugin-esbuild": "^4.9.3",
    "rollup-plugin-terser": "^7.0.2",
    "rxjs": "^7.5.6",
    "treeify": "^1.1.0",
    "uuid": "^9.0.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@sanity/pkg-utils": "workspace:*",
    "@sanity/semantic-release-preset": "^2.0.0",
    "@types/babel__core": "^7.1.19",
    "@types/cpx": "^1.5.2",
    "@types/find-config": "^1.0.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.7.13",
    "@types/prettier": "^2.7.0",
    "@types/rimraf": "^3.0.2",
    "@types/treeify": "^1.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "commitizen": "^4.2.5",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1"
  },
  "peerDependencies": {
    "typescript": "^4.7"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/pkg-utils.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/pkg-utils/issues"
  },
  "homepage": "https://github.com/sanity-io/pkg-utils#readme",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@7.12.2",
  "pnpm": {
    "overrides": {
      "@sanity/pkg-utils": "workspace:*"
    }
  }
}
