{
  "name": "exports-dummy",
  "version": "1.0.0",
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.js": "./dist/index.browser.js"
  },
  "exports": {
    ".": {
      "browser": {
        "source": "./src/index.ts",
        "require": "./dist/index.browser.cjs",
        "import": "./dist/index.browser.js"
      },
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./extra": {
      "browser": {
        "source": "./src/extra.ts",
        "require": "./dist/extra.browser.cjs",
        "import": "./dist/extra.browser.js"
      },
      "source": "./src/extra.ts",
      "require": "./dist/extra.cjs",
      "import": "./dist/extra.js",
      "types": "./dist/extra.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "extra": ["./dist/extra.d.ts"]
    }
  },
  "files": [
    "dist",
    "extra.js"
  ],
  "scripts": {
    "build": "node -r esbuild-register ./node_modules/@sanity/pkg-utils/src/cli build --extract --tsconfig tsconfig.dist.json"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "esbuild": "^0.15.5",
    "esbuild-register": "^3.3.3",
    "typescript": "^4.7.4"
  }
}
